<h2 class="text-center">Simple Calculator</h2>

@model CalculatorApp.Models.CalculatorModels
@{
    Layout = null;
}
@using (Html.BeginForm())
{
    <fieldset>
        <legend style = 'color:black;text-align:center'> Operation Of Two Numbers</legend>
            @Html.EditorFor(x => x.A) <br/> <br/> @Html.EditorFor(x => x.B) <br/>
            <br/> <input type = "submit"
                value = "add"
                name = "command" /> <input type = "submit"
                value = "sub"
                name = "command" /> <input type = "submit"
                value = "mul"
                name = "command" /> <input type = "submit"
                value = "div"
                name = "command" /> <br/>
            <br/> <h2 style = "color:black"> 
            @Html.Label("Result is :")
            @Html.DisplayFor(x => x.Result) </h2>
    </fieldset>
}

